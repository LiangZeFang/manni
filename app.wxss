/**app.wxss**/
@import "/wxParse/wxParse.wxss";
@import "weui.wxss";
@import "/widget/zanui-quantity/index.wxss";
@import "/tabbar/tabbar.wxss";
@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_963141_2wopp8bkvfw.eot?t=1544607408048'); /* IE9*/
  src: url('//at.alicdn.com/t/font_963141_2wopp8bkvfw.eot?t=1544607408048#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
  url('//at.alicdn.com/t/font_963141_2wopp8bkvfw.ttf?t=1544607408048') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('//at.alicdn.com/t/font_963141_2wopp8bkvfw.svg?t=1544607408048#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-yuan:before { content: "\e656"; }

.icon-xuanze:before { content: "\e645"; }

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.navigator-hover {
    background: none;
    opacity: 1;
}
button::after{
border: none;
}
.no-list {
    position: absolute;
    display: inline-block;
    top: 50%;
    left: 50%;
    text-align: center;
    transform: translate(-50%,-50%);
}

.image-box {
    width: 180rpx;
    height: 170rpx;
    margin: 0 auto;
}

.no-list .text{
    font-size: 32rpx;
    line-height: 80rpx;
    color: #969696;
}

.image-box image {
    width: 180rpx;
    height: 170rpx;
}

.see-button {
    display: inline-block;
    padding: 6rpx 30rpx;
    margin-top: 50rpx;
    border-radius: 14rpx;
    color: #969696;
    border: 1rpx solid #969696;
    font-size: 32rpx;
}
.hot-block{
  display: flex;
  flex-wrap: wrap;
  padding: 0px 30rpx;
}
.hot-block .hot-item:nth-of-type(odd){
 margin-right: 30rpx;
}
.hot-block .hot-item{
  margin-bottom: 50rpx;
}
.hot-block .hot-item .item-detail{
 margin-top: -110rpx;
 text-align: center;
 font-size: 30rpx;
 color: #5e5d5d;
}
.hot-block .hot-item .item-detail text:nth-of-type(1){
  margin-right: 15rpx;
  color: #000;
  font-size: 36rpx;
}
.hot-block .hot-item .item-detail text:nth-of-type(2){
  text-decoration: line-through;
}
.hot-item image{
  width: 327rpx;
  height: 395rpx;
    border: 1px solid #d5d5d5;
}
.hide {
    display: none!important;
}