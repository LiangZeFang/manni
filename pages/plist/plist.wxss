view {
    line-height: normal;
}
.product{background: #f9f9f9;min-height: 1200rpx}
.product .productmenu{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
  display: flex;
  white-space: nowrap;
  text-align: center;
  border-bottom:1px solid #f0efef; 
}
.product .productmenu .Y {
    border-bottom: 6rpx solid #585858;
    height: 76rpx;
    transition: all 2s;
}
.productmenu text{
  width: 180rpx;
  display: inline-block;
}
.productlist{
  padding-left: 30rpx;
  display: flex;
  flex-wrap: wrap;
  padding-top: 30rpx;
}
.productlist .item{
 margin-bottom: 30rpx;
 font-size: 28rpx;

}
.item view{
  padding-left: 20rpx;
}
.productlist image{
  width: 330rpx;
  margin-right: 15rpx;
  height: 330rpx;
}
.productlist image:nth-of-type(odd){
 margin-right: 30rpx;
}
.productlist .title {
  width: 312rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.price{
  color: #ee2222;
  font-size: 36rpx;
}
.productmenu .active {
  border-bottom: 2rpx solid #007bff;
}
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}
.weui-navbar__slider {
    position: absolute;
    top: 76rpx;
    width: 4em;
    height: 3px;
    background-color: #585858;
    -webkit-transition: -webkit-transform 0.3s;
    transition: -webkit-transform 0.3s;
    transition: transform 0.3s;
}
.navs {
    position: relative;
    background:#fff;
}
.navs .jieguo {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    font-size:32rpx;
}