}

.order-header-address .address-detail {
    font-size: 24rpx;
    font-weight: bold;
}

.dashed-line {
    position: relative;
    height: 10px;
    border-bottom: 1px dashed #efefef;
    overflow: visible;
}

.dashed-line::after {
    position: absolute;
    bottom: -10rpx;
    right: -10rpx;
    content: "";
    display: block;
    background-color: #f5f5f5;
    height: 20rpx;
    width: 20rpx;
    border-radius: 50%;
}

.dashed-line::before {
    position: absolute;
    left: -10rpx;
    bottom: -10rpx;
    content: "";
    display: block;
    background-color: #f5f5f5;
    height: 20rpx;
    width: 20rpx;
    border-radius: 50%;
}

.order-header-logo {
    width: 100rpx;
}

.order-header-logo image {
    height: 100rpx;
    width: 100%;
}
.Rel {
    position: relative;
}
.Rel text:first-of-type{
    margin-right: 30rpx;
}
.Rel text:last-of-type{
    position:absolute;
    bottom:-22rpx;
    right:0;
    font-size:36rpx;
    font-family:cursive;
    color:#3b3b3b;
