.shadow {
    opacity: 0;
    display: none;
}

.selected {
    bottom: -130%;
    overflow: visible !important;
    margin: 0 40rpx;
    border-radius: 10rpx 10rpx 0 0;
    position: fixed;
    background: rgba(255, 255, 255, 0.8);
}

.fadeIn {
    animation: fadeIn 0.3s infinite;
    animation-iteration-count: 1;
    display: block;
    opacity: 1;
}

@keyframes fadeIn {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}

.slideUp {
    animation: slideUp 0.5s infinite;
    animation-iteration-count: 1;
    bottom: 30%;
}

@keyframes slideUp {
    0% {
        bottom: 0;
    }

    100% {
        bottom: 30%;
    }
}

.fadeOuts {
    animation: fadeOuts 0.3s infinite;
    animation-iteration-count: 1;
    display: none;
}

@keyframes fadeOuts {
    0% {
        opacity: 1;
    }

    100% {
        opacity: 0;
    }
}

.slideDown {
    animation: slideDown 0.3s infinite;
    animation-iteration-count: 1;
    bottom: -130%;
}

@keyframes slideDown {
    0% {
        bottom: 0;
    }

    100% {
        bottom: -130%;
    }
}
